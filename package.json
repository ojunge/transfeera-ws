{
  "name": "transfeera-ws",
  "version": "1.0.0",
  "description": "web service para projeto da Transfeera",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose --coverage --forceExit --watchAll --maxWorkers=1",
    "db:create": "sequelize-cli db:create",
    "db:migrate": "sequelize-cli db:migrate",
    "db:g:migration": "sequelize-cli migration:generate --name",
    "db:g:seed": "sequelize-cli seed:generate --name",
    "db:seeds": "sequelize-cli db:seed:all",
    "start": "NODE_ENV=development node ./src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ojunge/transfeera-ws.git"
  },
  "keywords": [
    "node",
    "express"
  ],
  "author": "Ojunge",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ojunge/transfeera-ws/issues"
  },
  "homepage": "https://github.com/ojunge/transfeera-ws#readme",
  "dependencies": {
    "ak-logger": "^1.1.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-promise-router": "^3.0.3",
    "joi": "^17.4.0",
    "moment": "^2.29.1",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.19.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^27.0.4",
    "sequelize-cli": "^5.5.1",
    "superagent": "^6.1.0",
    "supertest": "^6.1.3"
  }
}
